.pagebox{
  width: 100%;
  height: 100vh;
  /* background-image: linear-gradient(#11998e, #38ef7d); */
  background-color: #a8c0ff;
  display: flex;
  flex-wrap: wrap;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.item{
  width: 80%;
  height: 120rpx;
  border-radius: 24rpx;
  background-color: #13b4f2;
  text-align: center;
  line-height: 120rpx;
  color: #fff;
  letter-spacing: 4rpx;
  margin-bottom: 40rpx;
}








