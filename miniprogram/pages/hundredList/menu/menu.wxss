/* pages/hundredList/menu/menu.wxss */
.pagebox{
  width: 100%;
  height: 100vh;
  box-sizing: border-box;
  padding: 24rpx 32rpx;
  background-color: #FFFFCC;
}
.search{
  display: flex;
  align-items: center;
}
.searchbox{
  width: 80%;
  height: 88rpx;
  border: 1px solid #ccc;
  border-top-left-radius: 12rpx;
  border-bottom-left-radius: 12rpx;
}
.searchbtn{
  flex:1;
  height: 88rpx;
  background-color: #FF6666;
  border-top-right-radius: 12rpx;
  border-bottom-right-radius: 12rpx;
  border: 1px solid #ccc;
  border-left: none;
  text-align: center;
  line-height: 88rpx;
  color: #fff;
  letter-spacing: 6rpx;
}
.result{
  box-sizing: border-box;
  padding: 24rpx;
  margin-top: 60rpx;
  width: 100%;
  height: 80vh;
  overflow-y: scroll;
  background-color: #FFCC99;
  border-radius: 12rpx;
}
.item{
  box-sizing: border-box;
  width: 100%;
  margin-bottom: 50rpx;
  color: #006699;
}
.title{
  width: 100rpx;
 color: #6699FF ;
}
.van-cell{
  height: 88rpx;
  border-top-left-radius: 12rpx;
  border-bottom-left-radius: 12rpx;
}