/* pages/hundredList/story/story.wxss */
.pagebox{
  width: 100%;
  height: 100vh;
  box-sizing: border-box;
  padding: 24rpx 32rpx;
  background-color: #FFCC99;
}
.btn{
  margin: 0 auto;
  width: 80%;
  height: 88rpx;
  border-radius: 44rpx;
  text-align: center;
  line-height: 88rpx;
  background-color: #66CCCC;
  letter-spacing: 8rpx;
  color: #336699;
}
.result{
  box-sizing: border-box;
  padding: 24rpx;
  margin-top: 60rpx;
  width: 100%;
  height: 80vh;
  overflow-y: scroll;
  overflow-x: hidden;
  background-color: #3399CC;
  border-radius: 12rpx;
}
.title{
  width: 100%;
  text-align: center;
  height: 60rpx;
  font-weight: bold;
  color: #FFFF00;
  letter-spacing: 8rpx;
}
.con{
  color:#FFFF00;
  line-height: 1.4;

}