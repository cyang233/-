/* pages/hundredList/eazy/eazy.wxss */
.pagebox{
  width: 100%;
  height: 100vh;
  box-sizing: border-box;
  padding: 24rpx 32rpx;
  background-color: #CCCCFF;
  display: flex;
  justify-content: center;
  align-items: center;
}
.content{
  box-sizing: border-box;
  padding: 24rpx 32rpx;
  width: 100%;
  height: 60vh;
  background-color: #99CCFF;
  border-radius: 12rpx;
  display: flex;
  flex-direction:column;
  align-items: center;
  justify-content: space-around;
}
.con{
  box-sizing: border-box;
  width: 100%;
  height: 88rpx;
  background-color:#CCFFFF;
}
.result{
  padding:24rpx 32rpx;
  line-height:1.5;
  height: 160rpx;
  border-radius: 12rpx;
  overflow-y: scroll;

}
.btn{
  width: 162rpx;
  height: 92rpx;
  text-align: center;
  line-height: 92rpx;
  background-color: #66CC99;
  border-radius: 162rpx;
  letter-spacing: 6rpx;
  color: #FFFFCC;
}
.van-cell{
  height: 88rpx;
  border-top-left-radius: 12rpx;
  border-bottom-left-radius: 12rpx;
}
